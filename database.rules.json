{
  "rules": {
    "notes": {
      ".read": true,
      ".write": true,
      "$noteId": {
        ".validate": "newData.hasChildren(['text','timestamp']) &&
                      newData.child('text').isString() &&
                      newData.child('timestamp').isNumber() &&
                      newData.child('text').val().length <= 20000"
      }
    }
  }
}
